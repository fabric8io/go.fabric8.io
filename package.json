{
  "name": "gofabric8",
  "version": "0.0.1",
  "description": "Deploy fabric8 from your browser",
  "homepage": "https://github.com/fabric8/go.fabric8.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabric8/go.fabric8.io.git"
  },
  "author": {
    "name": "Jimmi Dyson",
    "email": "jimmidyson@gmail.com"
  },
  "license": "Apache 2.0",
  "private": true,
  "engines": {
    "node": ">=5.2"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --display-modules --progress",
    "clean": "del-cli ./dist",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "lint": "eslint -c .eslintrc.yml *.js ./src ./server",
    "lint:fix": "eslint -c .eslintrc.yml --fix *.js ./src ./server",
    "prebuild": "npm-run-all clean lint",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' ./server/main.js",
    "start": "npm run dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.6",
    "babel-core": "~6.9.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "~6.2.4",
    "babel-plugin-react-transform": "~2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "~6.9.0",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bootstrap-loader": "^1.0.10",
    "bootstrap-sass": "^3.3.6",
    "classnames": "~2.2.5",
    "core-decorators": "^0.12.3",
    "cross-env": "~1.0.7",
    "css-loader": "~0.23.1",
    "del-cli": "~0.2.0",
    "eslint": "^2.13.1",
    "eslint-config-kubism": "^1.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "~5.1.1",
    "expose-loader": "^0.7.1",
    "express": "~4.13.4",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "^0.8.5",
    "firebase": "~3.0.2",
    "history": "~2.1.1",
    "html-webpack-plugin": "~2.17.0",
    "imports-loader": "^0.6.5",
    "jasmine-core": "~2.4.1",
    "jquery": "^3.0.0",
    "js-yaml": "^3.6.1",
    "karma": "~0.13.22",
    "karma-chrome-launcher": "~1.0.1",
    "karma-jasmine": "~1.0.2",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.7.0",
    "mobx": "^2.3.2",
    "mobx-connect": "^0.1.0",
    "mobx-react": "^3.3.1",
    "mobx-react-devtools": "^4.2.0",
    "morgan": "~1.7.0",
    "node-sass": "~3.7.0",
    "nodemon": "~1.9.2",
    "npm-run-all": "~2.1.1",
    "postcss-loader": "~0.9.1",
    "react": "~15.1.0",
    "react-addons-test-utils": "~15.1.0",
    "react-dom": "~15.1.0",
    "react-router": "~2.4.1",
    "react-transform-hmr": "~1.0.4",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "~3.2.0",
    "sinon": "~1.17.4",
    "style-loader": "~0.13.1",
    "urijs": "^1.18.1",
    "url-loader": "^0.5.7",
    "webpack": "~1.13.1",
    "webpack-dev-server": "~1.14.1",
    "whatwg-fetch": "^1.0.0",
    "winston": "~2.2.0"
  }
}
